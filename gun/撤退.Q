[Comment]
撤退技能位置4
减少使用次数可以装弹……
所以，预多次数吧……

[Attachment]



[Script]
UserVar FoX = 0 "X坐标"
UserVar FoY = 0 "Y坐标"
UserVar FoW = 1024 "W坐标"
UserVar FoH = 768 "H坐标"
UserVar ForNumber = 9999 "循环次数"
For ForNumber
    Call CLCD
    If CLOK = 1 Then 
        KeyPress "4", 1
        Delay 1400
        KeyDown "Ctrl", 1
        Delay 100
        MoveTo (FoX + FoW) / 2, (FoY + FoH) / 2
        Delay 200
        LeftClick 1
        Delay 100
        KeyUp "Ctrl", 1
        CLOK = 0
        
        Rem 技能进行中
        Call CLing
        If CL持续中 = 15 Then 
            KeyDown "Ctrl", 1
            Delay 100
            MoveTo (FoX + FoW) / 2, (FoY + FoH) / 2
            Delay 200
            LeftClick 1
            Delay 100
            KeyUp "Ctrl", 1

        End If
        If CL中 = 1
            Delay 1000
            Goto 技能进行中
            CL持续中=CL持续中+1
        Else 
            Delay 1000
        End If
    End If
    CL持续中=0
	
	
Next

EndScript

Sub CLCD
    FindPic FoX,FoY,FoW,FoH,"Attachment:\CL.bmp",1,intX,intY
    If intX > FoX And intY > FoY Then 
        CLOK = 1
    Else 
        CLOK=0
    End If

End Sub
Sub CLing
    FindPic FoX,FoY,FoW,FoH,"Attachment:\CLing.bmp",1,intX,intY
    If intX > FoX And intY > FoY Then 
        CL中 = 1
    Else 
        CL中 = 0
    End If

End Sub
