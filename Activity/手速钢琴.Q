[Attachment]



[Script]
UserVar FoX = 0 "X坐标"
UserVar FoY = 0 "Y坐标"
UserVar FoW = 1024 "W坐标"
UserVar FoH = 768 "H坐标"
UserVar Del = 80 "延迟/ms"
UserVar FB_Mod = Droplist{"范围":1|"全屏":2}=2 "执行范围"

If FB_Mod = 1 Then 
    SX = FoX
    SY = FoY
    SW = FoW
    SH = FoH
ElseIf FB_Mod = 2 Then
    SX = 0
    SY = 0
    SW = Plugin.Sys.GetScRX()
    SH = Plugin.Sys.GetScRY()
End If


While a=0
    Call Plugin.ColorEx.PrintScreen(SX, SY, SW, SH)
    XY = Plugin.ColorEx.FindColor(SX, SY, SW, SH, "517497", 1, 0)
    ZB = InStr(XY, "|")
    X = Clng(Left(XY, ZB - 1)) : Y = Clng(Right(XY, Len(XY) - ZB))
    If X>SX and Y>SY
        MoveTo X , Y 
        Delay 80
        LeftClick 1
    Else
        XY = Plugin.ColorEx.FindPic(SX, SY, SW, SH, "D:\1.bmp", 1, 0)
        ZB = InStr(XY, "|")
        X = Clng(Left(XY, ZB - 1)): Y = Clng(Right(XY, Len(XY) - ZB))      
        If X > 0 and Y > 0 Then 
        MoveTo X + 6, Y + 5
        Delay Del
        LeftClick 1
        End If

        XY = Plugin.ColorEx.FindPic(SX, SY, SW, SH, "D:\2.bmp", 1, 0)
        ZB = InStr(XY, "|")
        X = Clng(Left(XY, ZB - 1)): Y = Clng(Right(XY, Len(XY) - ZB))      
        If X > 0 and Y > 0 Then 
        MoveTo X + 6, Y + 5
        Delay Del
        LeftClick 1
        End If
        
        XY = Plugin.ColorEx.FindPic(SX, SY, SW, SH, "D:\开始.bmp", 1, 0)
        ZB = InStr(XY, "|")
        X = Clng(Left(XY, ZB - 1)): Y = Clng(Right(XY, Len(XY) - ZB))      
        If X > 0 and Y > 0 Then 
        MoveTo X + 26, Y + 13
        Delay Del
        LeftClick 1
        Delay 1000

        End If

    End If
    
    Call Plugin.ColorEx.Free()
    Delay 100
Wend

