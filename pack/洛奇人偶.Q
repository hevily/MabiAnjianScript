BeginHotkeyMod=0
PauseHotkey=0
PauseHotkeyMod=0
StopHotkey=32775
StopHotkeyMod=0
RunOnce=1
EnableWindow=
MacroID=e424349a-329f-4f98-83f1-c7a522fa9baf
Description=洛奇人偶
Enable=0
AutoRun=0
[Repeat]
Type=0
Number=1
[SetupUI]
Type=2
QUI=
[Relative]
SetupOCXFile=
[Comment]
操纵：1
1幕：2
2幕：3
牵引：9

[Attachment]



[Script]
UserVar VarSM = droplist{"1幕":1|"2幕":2|"操纵":4|"牵引":5|"捆绑":6} = 1 "模式选择"
UserVar FoX = 0 "X坐标"
UserVar FoY = 0 "Y坐标"
UserVar FoW = 1024 "W坐标"
UserVar FoH = 768 "H坐标"
UserVar VarNum = 100 "循环次数"
SaveMousePos 
UserVar VarS = 300 "延迟/毫秒"


If VarSM = 1 
    For VarNum
        Rem SM1
        Call Y一幕CD
        If Y一幕OK = 1 Then 
            Delay 500
            RestoreMousePos 

            KeyPress "2", 1
            Delay 1900

            LeftClick 1
            Delay 3000
            
            Y一幕OK = 0
        Else 
            Delay 1000
            Goto SM1
        End If
    Next
End If


If VarSM = 2 Then 
    For VarNum
        Rem SM2
        Call E二幕CD 
        If E二幕OK = 1
            Delay 700
            RestoreMousePos

            KeyPress "3", 1
            Delay 1900
            Delay VarS
            LeftClick 1
            Delay 6000

            
            E二幕OK = 0
        Else 
            Delay 1000

            Goto SM2
        End If

    Next
End If



If VarSM = 4 Then 
    For VarNum
        Rem SM4
        Delay 200

        Call C操纵CD
        If C操纵OK = 1 Then 
            Delay 300
            KeyPress "1", 1
            
            C操纵OK = 0
        Else 
            Goto SM4
        End If
    Next

End If 

If VarSM = 5 Then 
    For VarNum
        Rem SM5
        Delay 500
        Call Q牵引CD
        If Q牵引OK = 1 Then 
            Delay 500
            KeyPress "9", 1
            Delay VarS
            KeyPress "E", 1
            Delay 1500
            
            Q牵引OK = 0
        Else 
            Goto SM5
        End If
    
    Next
End If

If VarSM = 6 Then 
    For VarNum
        Rem SM6
        Delay 500
        Call Q捆绑CD
        If Q捆绑OK = 1 Then 
            Delay 500
            KeyPress "0", 1
            Delay VarS
            KeyPress "E", 1
            Delay 1000
            
            Q捆绑OK = 0
        Else 
            Goto SM6
        End If
    
    Next
End If

Sub Y一幕CD
    FindPic FoX,FoY,FoW,FoH,"Attachment:\一幕.bmp",1,intX,intY
    If intX > FoX And intY > FoY Then 
        Y一幕OK = 1
    End If

End Sub

Sub E二幕CD
    FindPic FoX,FoY,FoW,FoH,"Attachment:\二幕.bmp",1,intX,intY
    If intX > FoX And intY > FoY Then 
        E二幕OK = 1
    End If

End Sub

Sub C操纵CD
    FindPic FoX,FoY,FoW,FoH,"Attachment:\操纵.bmp",1,intX,intY
    If intX > FoX And intY > FoY Then 
        C操纵OK = 1
    End If
          
End Sub

Sub Q牵引CD
    FindPic FoX,FoY,FoW,FoH,"Attachment:\牵引.bmp",1,intX,intY
    If intX > FoX And intY > FoY Then 
        Q牵引OK = 1
    End If

End Sub

Sub Q捆绑CD
    FindPic FoX,FoY,FoW,FoH,"Attachment:\捆绑.bmp",1,intX,intY
    If intX > FoX And intY > FoY Then 
        Q捆绑OK = 1
    End If

End Sub
