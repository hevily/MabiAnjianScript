[Attachment]



[Script]
UserVar FoX = 0 "X坐标"
UserVar FoY = 0 "Y坐标"
UserVar FoW = 1600 "W坐标"
UserVar FoH = 900 "H坐标"
UserVar FB_Mod = Droplist{"填写":1|"自动":2}=2 "分辨率"
Uservar Mode =droplist{"准备工作":-1|"忍耐":1|"战争":2|"催眠":3|"丰收":4|"迷惑":5|"活跃":6|"行径":7}=1 "技能选择"
UserVar Del = 100 "搜索延迟/ms"
UserVar Del_RN = 800 "忍耐持续/ms"
UserVar  Del_ZZ = 2100 "战争持续/ms"
UserVar Del_CM = 600 "催眠持续/ms"
UserVar Del_FS = 4600 "丰收持续/ms"
UserVar Del_MH = 10000 "迷惑持续/ms"
UserVar Del_HY = 1900 "活跃持续/ms"
UserVar Del_XJ = 900 "行径持续/ms"

If FB_Mod = 1 Then 
    SX = FoX
    SY = FoY
    SW = FoW
    SH = FoH
ElseIf FB_Mod = 2 Then
    SX = 0
    SY = 0
    SW = Plugin.Sys.GetScRX()
    SH = Plugin.Sys.GetScRY()
End If

If Mode = -1 Then 
    PutAttachment "D:\Music_v1.0_Temp", "*.bmp"
    Call Plugin.Msg.Tips("文件释放成功！")
    EndScript
End If

If Mode = 1 Then 
    While a = 0
        Call Plugin.ColorEx.PrintScreen(SX, SY, SW, SH)
        Delay Del
        XY = Plugin.ColorEx.FindPic(SX, SY, SW, SH, "D:\Music_v1.0_Temp\忍耐.bmp", 1, 0)
        ZB = InStr(XY, "|")
        X = Clng(Left(XY, ZB - 1)) : Y = Clng(Right(XY, Len(XY) - ZB))
        If X > 0 And Y > 0 Then 
            KeyPress "4", 1
            Delay Del_RN
            KeyPress "Esc", 1
        End If
    Wend
End If

