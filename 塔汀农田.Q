[Script]
UserVar JSNum = 4 "浇水次数"
UserVar SFNum = 4 "施肥次数"
UserVar ZCNum = 2 "捉虫次数"
UserVar FoX = 0 "X坐标"
UserVar FoY = 0 "Y坐标"
UserVar FoW = 1024 "W坐标"
UserVar FoH = 768 "H坐标"

For JSNum
    REM JSAG
    Call JSCD
    If JSOK = 1 Then 
        MoveTo intX + 12, intY + 13
        Delay 500
        LeftClick 1
        Delay 1000
        MoveTo FoX, FoY
        Delay 60000
        JSOK = 0
    
    Else 
        Delay 20000
        Goto JSAG
    End If
Next

For SFNum
    REM SFAG
    Call SFCD
    If SFOK = 1 Then 
        MoveTo intX + 12, intY + 13
        Delay 500
        LeftClick 1
        Delay 1000
        MoveTo FoX, FoY
        Delay 60000
        SFOK = 0
    
    Else 
        Delay 20000
        Goto SFAG
    End If
Next

For ZCNum
    REM ZCAG
    Call ZCCD
    If ZCOK = 1 Then 
        MoveTo intX + 12, intY + 13
        Delay 500
        LeftClick 1
        Delay 1000
        MoveTo FoX, FoY
        Delay 60000
        SFOK = 0
    
    Else 
        Delay 20000
        Goto ZCAG
    End If
Next

MsgBox "已经完成！" &vbcr& JSNum &"次浇水" &vbcr & SFNum &"次施肥" &vbcr &ZCNum & "次捉虫"
EndScript




Sub JSCD
    FindPic FoX,FoY,FoW,FoH,"Attachment:\JS.bmp",1,intX,intY
    If intX > FoX And intY > FoY Then 
        JSOK = 1
    End If

End Sub
Sub SFCD
    FindPic FoX,FoY,FoW,FoH,"Attachment:\SF.bmp",1,intX,intY
    If intX > FoX And intY > FoY Then 
        SFOK = 1
    End If

End Sub
Sub ZCCD
    FindPic FoX,FoY,FoW,FoH,"Attachment:\ZC.bmp",1,intX,intY
    If intX > FoX And intY > FoY Then 
        ZCOK = 1
    End If

End Sub
