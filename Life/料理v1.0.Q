[Comment]
自备材料在背包，能直接看见，不会被料理技能框所挡
切换风格为 电子产品 不透明

[Attachment]



[Script]
UserVar Var1 = 1 "制作份数"
UserVar Var2 =droplist{"蔬菜面包":1|"番茄罗勒色拉":2|"金枪鱼拌菜(0品质)":3|"BnR":4} = 1 "技能选择"

If Var2 = 1 Then 
    For var1
        //    蔬菜面包
        KeyPress "-", 1
        Delay 1000
        
        //        查找料理窗口
        FindPic 0,0,1024,1024,"Attachment:\料理窗口.bmp",0.7,intX2,intY2

        //煮的土豆
        FindPic 0,0,1024,1024,"Attachment:\td.bmp",0.8,intXw,intYw
        If intXw > 0 And intYw > 0 Then 
            Delay 50
            Moveto intXw+2,intYw+2
            Delay 200
            LeftClick 1
            Delay 280
            Delay 500
            MoveTo intX2 + 40, intY2 - 25
            Delay 500
            LeftClick 1
        End If


        //胡萝卜
        FindPic 0,0,1024,1024,"Attachment:\hlb.bmp",0.7,intXw,intYw
        If intXw > 0 And intYw > 0 Then
            Delay 600
            MoveTo intXw + 2, intYw + 2
            Delay 800
            KeyDown "Shift", 1
            Delay 800
            LeftClick 1
            Delay 800
            KeyUp "Shift", 1
            Delay 800
            //    判断数量是否大于1，做处理
            FindPic 0,0,1024,1024,"Attachment:\分割窗口.bmp",0.7,intX,intY
            If intX > 0 And intY > 0 Then 
                Delay 1000

                KeyPress "Enter", 1

                Goto 萝卜1
            End If
            LeftClick 1
            Rem 萝卜1
    
    
            Delay 800
            MoveTo intX2 + 120, intY2 - 25
            Delay 900
            LeftClick 1

        End If
        //柠檬
        FindPic 0,0,1024,1024,"Attachment:\柠檬.bmp",0.7,intXw,intYw
        If intXw > 0 And intYw > 0 Then 
            MoveTo intXw + 3, intYw + 3
            Delay 800
            KeyDown "Shift", 1
            Delay 800
            LeftClick 1
            Delay 800
            KeyUp "Shift", 1
            Delay 800
            //    判断数量是否大于1，做处理
            FindPic 0,0,1024,1024,"Attachment:\分割窗口.bmp",0.7,intX,intY
            If intX > 0 And intY > 0 Then 
                Delay 1000

                KeyPress "Enter", 1
                Goto 柠檬1
            End If
            LeftClick 1
            Rem 柠檬1
    
            Delay 800
            MoveTo intX2 + 210, intY2 - 25
            Delay 900
            LeftClick 1
            Delay 1100


        End If   
        //    开始配比  萝卜50  土豆40  柠檬10
        //First，土豆的40 
        MoveTo intX2 + 40, intY2 + 8
        Delay 800
        LeftDown 1
        Delay 1000
        LeftUp 1
        //                  土豆40细微配比
        Rem 土豆40配比
        FindPic 0,0,1024,1024,"Attachment:\土豆的40%.bmp",1,intXpb,intYpb
        If intXpb > 0 And intYpb > 0 Then 
            Goto 土豆40配比完成
        End If
        LeftDown 1
        Delay 100
        LeftUp 1
        Goto 土豆40配比
        Rem 土豆40配比完成
        
        //Second，萝卜的50 
        Rem 土豆40和萝卜50配比
        MoveTo intX2 + 120, intY2 + 8
        FindPic 0,0,1024,1024,"Attachment:\土豆的40%+萝卜的50%.bmp",1,intXpb,intYpb
        If intXpb > 0 And intYpb > 0 Then 
            Goto 土豆40和萝卜50配比完成
        End If
        LeftDown 1
        Delay 100
        LeftUp 1
        Goto 土豆40和萝卜50配比
        Rem 土豆40和萝卜50配比完成
        
        //Third，柠檬的10 
        Rem 柠檬10配比
        MoveTo intX2 + 210, intY2 +8
        FindPic 0,0,1024,1024,"Attachment:\完成配比.bmp",1,intXpb,intYpb
        If intXpb > 0 And intYpb > 0 Then 
            Goto 蔬菜面包完成
        End If
        LeftDown 1
        Goto 柠檬10配比
        Rem 蔬菜面包完成
        Delay 100

        LeftUp 1

        //        点击 开始料理
        Delay 1000
        MoveTo intX2 + 82, intY2 +75
        Delay 1000
        LeftClick 1
        Delay 7000

    endfor
End If

