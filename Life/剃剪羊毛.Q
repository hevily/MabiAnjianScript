            Delay 300
            MoveTo YMX + 2, YMY + 2
            Delay 300
            LeftClick 1
            Delay 300
        End If
    endfor
    KeyDown "Alt", 1
    Delay 100
    FindPic FoX,FoY,FoW,FoH,"Attachment:\red.bmp",1,RX,RY
    If RX > 0 And RY > 0 Then 
        Delay 800
        FindPic FoX,FoY,FoW,FoH,"Attachment:\割毛刀.bmp",0.95,XDX,XDY
        If XDX > 0 And XDY > 0 Then 
            Delay 500

            MoveTo XDX + 6, XDY + 15
            Delay 500
            KeyDown "Ctrl", 1
            Delay 100
            LeftClick 1
            Delay 200
            KeyUp "Ctrl", 1
        End If
    End If

